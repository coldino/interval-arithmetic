    "connected",
    "set",
    "real",
    "interval",
    "arithmetic"
  ],
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.2",
    "doctoc": "^0.13.0",
    "eslint": "^0.20.0",
    "istanbul": "^0.3.8",
    "mocha": "^2.2.1",
    "mocha-lcov-reporter": "^0.0.2",
    "nodemon": "^1.3.7"
  },
  "scripts": {
    "istanbul": "istanbul cover _mocha -- -R spec test/index.js",
